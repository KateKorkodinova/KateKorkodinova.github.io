<IfModule dir_module>
    DirectoryIndex index.php index.html
</IfModule>
AddDefaultCharset UTF-8
AddCharset UTF-8 .html
<FilesMatch "\.(html)$">
   Header set Cache-Control: "no-cache, no-store"
   Header unset ETag
</FilesMatch>
Header set X-Content-Type-Options nosniff
ErrorDocument 404 http://localhost/library/404.html
## EXPIRES CACHING ##
<IfModule mod_expires.c>
    ExpiresActive On
    ExpiresByType img/jpg "access plus 1 year"
    ExpiresByType img/jpeg "access plus 1 year"
    ExpiresByType img/gif "access plus 1 year"
    ExpiresByType img/png "access plus 1 year"
    ExpiresByType txt/css "access plus 1 month"
    ExpiresByType aplication/pdf "access plus 1 month"
    ExpiresByType text/x-javascript "access plus 1 month"
    ExpiresByType aplication/x-shockwave-flash "access plus 1 month"
    ExpiresByType img/x-icon "access plus 1 year"
    ExpiresDefault "access plus 7 days"
</IfModule>
## EXPIRES CACHING ##
